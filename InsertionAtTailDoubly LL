#include <iostream>
using namespace std;
struct node{
    int data;
    node* next;
    node* prev;
    
    node(int value){
        data=value;
        next=nullptr;
        prev=nullptr;
        
    }
};
void PrintDoubly(node* head){
    node* p=head;
    
    while(p!=nullptr){
        cout<<p->data<<" ";
        p=p->next;
        
    }
}
void insertionInDoubly(node* head,int pos,int val){
    node*p=head;
    node* q=nullptr;
    
    for(int i=1;i<pos;i++){
        q=p;
        p=p->next;
    }
    node* newNode=new node(val);
    newNode->next=p;
    q->next=newNode;
    p->prev=newNode;
    newNode->prev=q;
    
}
void insertionDoublyTail(node*head,int val){
    node*p=head;
    node* newNode=new node(val);
    
    while(p->next!=nullptr){
        p=p->next;
    }
    p->next=newNode;
    newNode->prev=p;
}
int main()
{
    node* head=new node(2);
    head->next=new node(3);
    head->next->prev=head;
    head->next->next=new node(5);
    head->next->prev=head->next;
    
    //int pos=3;
    int value=9;
    
   // insertionInDoubly(head,pos,value);
    insertionDoublyTail(head,value);
    PrintDoubly(head);
    
    return 0;
}
