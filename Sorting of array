#include<iostream>
using namespace std;

int main(){
    int n=5;
    int arr[n]={2,4,3,1,5};
    
    
    for(int i=0;i<n;i++){
        for(int j=0;j<n-1;j++){
            if(arr[j]>arr[j+1]){
                swap(arr[j],arr[j+1]);
            }
        }
        
        
    }
    cout << "Sorted Array in Ascending Order: ";
    for(int i = 0; i < 5; i++) {
        cout << arr[i] << " ";
    }
    
}
