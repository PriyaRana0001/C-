#include<bits/stdc++.h>
using namespace std;

class Stack{
    public:
    int top;
    int arr[100];
    
    Stack(){
        top=-1;
    }
    
    void push(int val){
        if(top==100){
            cout<<"stack is full"<<endl;
        
        }
        else{
            top++;
            arr[top]=val;
        }
    }
    void pop(){
        if(top==-1){
            cout<<"Stack is empty"<<endl;
        }
        else{
            top--;
        }
    }
    int peek(){
        if(top == -1){
            return -1;
        }else return arr[top];
        
    }
    bool isEmpty(){
        if(top == -1){
            return true;
        }else{
            return false;
        }
    }
    void traversal(){
        if(top == -1){
            cout<<"stack is empty"<<endl;
        }
        else{
            int i=top;
            while(i>=0){
                cout<<arr[i]<<" ";
                i--;
            }
        }
    }
};

int main()
{
    Stack st;
    st.push(10);
    st.push(20);
    st.push(30);
    st.push(40);
    st.push(50);
    
    cout<<st.isEmpty()<<endl;
    st.pop();
    cout<<st.peek()<<endl;
    st.traversal();
    
    return 0;
}
