
#include <iostream>
using namespace std;
struct node{
    int data;
    node* next;
    
    node(int value){
        data=value;
        next=NULL;
    }
};
int countNum(node* head){
    node* p=head;
    int count=0;
    
    while(p!=NULL){
        count++;
        p=p->next;
    }
    return count;
}
void Print(node* head){
    node* p=head;
    
    while(p!=NULL){
        cout<<p->data<<" ";
        p=p->next;
    }
}

int main()
{
    node* head=new node(2);
    head->next=new node(5);
    head->next->next=new node(3);
    head->next->next->next=new node(9);
    head->next->next->next->next=new node(8);
    
    // Print(head);
    int totalCount=countNum(head);
    cout<<"Total Count Of Elemets Is: "<<totalCount; 
    return 0;
}
